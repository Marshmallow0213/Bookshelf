@model Textbox
<div class="marqee">
 <ul>
        @foreach (var Announcement in ViewBag.Announcements)
        {
            <li>
                @Announcement
            </li>
        }
 </ul>
</div>
@section topCSS{
    <link href="~/css/home/index.css" asp-append-version="true" rel="stylesheet" />
}
@if (User.Identity.IsAuthenticated)
{
    <input type="hidden" class="form-control" id="userId" value="@User.Identity.Name" readonly />
}
<partial name="~/Views/shared/AdminIndexPartial.cshtml" />