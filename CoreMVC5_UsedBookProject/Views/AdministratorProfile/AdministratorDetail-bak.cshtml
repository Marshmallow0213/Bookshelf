@model AdministratorUserViewModel
@{
    ViewData["Title"] = "管理員個人資料明細";
}

<h4 class="alert alert-danger">管理員個人資料明細</h4>


<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayName(Model.Name)
        </dt>
        <dd class="col-sm-9">@Html.Display(Model.Name)</dd>
        <dt class="col-sm-2">@Html.DisplayName(Model.PhoneNo)</dt>
        <dd class="col-sm-9">@Html.Display(Model.PhoneNo)</dd>
        <dt class="col-sm-2">@Html.DisplayName(Model.Email)</dt>
        <dd class="col-sm-9">@Html.Display(Model.Email)</dd>
    </dl>
</div>
<div>
    <a asp-action="AdminEdit" asp-route-id="@Model.Id" class="btn btn-info">編輯</a>
    <a asp-controller="AdministratorProfile" asp-action="AdministratorData">返回管理員控制中心</a>

</div>